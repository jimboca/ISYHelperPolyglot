<nodeDefs>

  <!-- === Camera Server Holds all Camera Nodes ========================================  -->

  <nodeDef id="CameraServer" nodeType="139" nls="CAMS">
    <sts>
      <st id="GV1" editor="FLOAT" />
      <st id="GV2" editor="I_INTEGER" />
      <st id="GV3" editor="I_INTEGER" />
    </sts>
    <cmds>
      <sends />
      <accepts>
	<cmd id="QUERY" />
	<!--  Enable motion alarm -->
        <cmd id="SET_FOSCAM_MJPEG">
          <p id="GV3" editor="I_FOSCAM_MJPEG" init="GV3" />
        </cmd>
      </accepts>
    </cmds>
  </nodeDef>

  <!-- === Foscam MJPEG Camera ============================================================  -->

  <nodeDef id="FoscamMJPEG" nodeType="139" nls="CAM">
    <sts>
      <st id="GV1" editor="BOOL" />
      <st id="GV2" editor="BOOL" />
      <st id="GV3" editor="BOOL" />
      <st id="GV4" editor="BOOL" />
      <st id="GV5" editor="I_ALMOS" />
      <st id="GV6" editor="BOOL" />
    </sts>
    <cmds>
      <sends />
      <accepts>
	<!--  Enable motion alarm -->
        <cmd id="SET_ALMOA">
          <p id="GV3" editor="BOOL" init="GV1" />
        </cmd>
	<!--  Send email for motion alarm -->
        <cmd id="SET_ALML">
          <p id="GV4" editor="BOOL" init="GV2" />
        </cmd>
	<!--  Motion Sensitivity: 0-9：high-low (but I think it is low-high) -->
        <cmd id="SET_ALMOS">
          <p id="GV5" editor="I_ALMOS" init="GV3" />
        </cmd>
	<!-- Enable alarm motion compensation or not when the light changes suddently，0：no，1：yes  -->
        <cmd id="SET_ALMOC">
          <p id="GV6" editor="BOOL" init="GV6" />
        </cmd>
	<!--   -->
        <cmd id="SET_POS">
          <p id="POS" editor="I_POS16" />
        </cmd>
	<cmd id="QUERY" />
      </accepts>
    </cmds>
  </nodeDef>

  <!-- === Camera Motion Node ============================================================  -->
  <nodeDef id="CamMotion" nodeType="139" nls="CAMNM">
    <sts>
      <st id="ST" editor="I_ONOFF" />
    </sts>
    <cmds>
      <sends>
        <cmd id="DON" />
        <cmd id="DOF" />
      </sends>
      <accepts>
	<cmd id="QUERY" />
      </accepts>
    </cmds>
  </nodeDef>

  <!-- === Some other Camera ============================================================  -->
  <nodeDef id="FCAM2" nodeType="139" nls="CAM">
    <sts>
      <st id="GV1" editor="I_INTEGER" />
      <st id="GV2" editor="I_INTEGER" />
      <st id="GV3" editor="I_INTEGER" />
      <st id="GV4" editor="I_INTEGER" />
    </sts>
    <cmds>
      <sends />
      <accepts>
	<cmd id="QUERY" />
      </accepts>
    </cmds>
  </nodeDef>


</nodeDefs>
